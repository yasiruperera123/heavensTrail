{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": ["warn"]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  